<?xml version="1.0" encoding="UTF-8"?>
<configuration status="WARN">

    <!-- Appenders -->
    <appenders>
        <Console name="console" target="SYSTEM_OUT">
            <PatternLayout pattern="|%-5p|%d|%c(%M:%L)%n%m%n" />
        </Console>
    </appenders>

    <loggers>
        <!-- Application Loggers -->
        <logger name="seo.dale.spring" level="debug"/>

        <!-- Root Logger -->
        <root level="info">
            <appender-ref ref="console" />
        </root>
    </loggers>

</configuration>